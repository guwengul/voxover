<section class="section section--5 section--contactsales spacing" id="contact-us">
  <h2>Contact Us</h2>
  
  <div class="section--5__form">
  <form action="/contact-us" method="POST">
  
  <div class="form__input form__firstname">
  <label for="firstname">First name</label>
  <br>
  <br>
  <input type="text" name="firstName" id="firstname">
  </div>
  
  <div class="form__input form__lastname">
  <label for="lastname">Last name</label>
  <br>
  <br>
  <input type="text" name="lastname" id="lastname">
  </div>
  
  <div class="form__input form__email">
  <label for="email">Work Email</label>
  <br>
  <br>
  <input type="text" name="email" id="email">
  </div>
  
  <div class="form__input form__email">
  <label for="usecase">Your use case?</label>
  <br>
  <br>
  <textarea name="useCase" id="usecase" rows="4" cols="auto"></textarea>
  </div>
  
  <div>
  <button class="btn btn--ctn-sales btn--submit" type="submit" disabled >Submit</button>
  </div>
  </form>
  </div>
  
  <!-- Success Dialog -->
  <div id="successModal" class="modal">
  <div class="modal-content">
  <p id="successMessage">Form submitted successfully!</p>
  <span class="close-btn" id="closeSuccessModal">&times;</span>
  </div>
  </div>
  
  <!-- Error Dialog -->
  <div id="errorModal" class="modal">
  <div class="modal-content">
  <p id="errorMessage">There was an error submitting the form. Please try again.</p>
  <span class="close-btn" id="closeErrorModal">&times;</span>
  </div>
  </div>
